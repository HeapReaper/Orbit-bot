{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@enums/*": ["./enums/*"],
      "@utils/*": ["./utils/*"],
      "@src/*": ["./src/*"],
    },
    "module": "esnext",
    "target": "es2020",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "allowImportingTsExtensions": true,
    "noEmit": true,
  }
}